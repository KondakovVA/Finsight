<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Background colors -->
    <Color x:Key="backColor1">#FF272E32</Color>
    <Color x:Key="backColor2">#FF4E5B63</Color>
    <Color x:Key="mainBackColor">#FFedebeb</Color>

    <!-- Border colors -->
    <Color x:Key="borderColor1">#FF4E5B63</Color>
    <Color x:Key="borderColor2">#FF272E32</Color>

    <!-- Brushes -->
    <SolidColorBrush x:Key="MainBackgroundBrush" Color="{StaticResource mainBackColor}"/>
    <SolidColorBrush x:Key="loginBtnColor" Color="#303340"/>
    <SolidColorBrush x:Key="loginBtnOverColor" Color="#21232b"/>
    <SolidColorBrush x:Key="commonBtnColor" Color="#0c237d"/>
    <SolidColorBrush x:Key="commonBtnOverColor" Color="#1234b8"/>
    <SolidColorBrush x:Key="commonBtnDisableColor" Color="#601234b8"/>

    <!-- Font colors -->
    <SolidColorBrush x:Key="titleColor1" Color="#E0E1F1"/>
    <SolidColorBrush x:Key="titleColor2" Color="#DBDBDB"/>
    <SolidColorBrush x:Key="titleColor4" Color="#272E32"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="#9497CD"/>

    <!-- Menu colors -->
    <SolidColorBrush x:Key="menuOverColor" Color="#474747"/>
    <SolidColorBrush x:Key="menuActiveColor" Color="#383838"/>
</ResourceDictionary>
